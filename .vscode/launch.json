  "version": "0.2.0",
  "configurations": [
    {
      "name": "meditation_maker web debug",
      "request": "launch",
      "type": "dart",
      "preLaunchTask": "flutter pub run build_runner build",
      "args": ["--device-id=edge", "--web-hostname=localhost", "--web-port=4850"]
    },
    {
      "name": "meditation_maker (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "meditation_maker (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
